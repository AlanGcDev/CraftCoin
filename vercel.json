{
    "builds": [
      {
        "src": "your_project_name/wsgi.py",
        "use": "@vercel/python",
        
        "source": "/feedback/((?!general).*)",
        "destination": "/api/feedback/general"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "your_project_name/wsgi.py"
      }
    ]
    
  }